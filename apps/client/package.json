{
  "name": "@starter-project/client",
  "version": "13.0.0",
  "private": true,
  "dependencies": {
    "@starter-project/entities": "workspace:*",
    "@starter-project/server-conn-info": "workspace:*",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-query": "^3.33.2",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "dotenv -- if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:dev": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "start:prod": "yarn serve",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "test:watch": "yarn test --watch",
    "watch": "yarn test",
    "eject": "react-scripts eject",
    "serve": "serve ./build -l ${PORT:-4000} -s -n",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll",
    "cy:open": "# yarn run cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.4.1",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-docs": "^6.4.1",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-links": "^6.4.1",
    "@storybook/node-logger": "^6.4.1",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/storybook__addon-a11y": "^5.1.2",
    "@types/styled-components": "^5.1.15",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.1",
    "if-env": "^1.0.4"
  }
}
